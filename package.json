{
    "name": "rebaser",
    "displayName": "rebaser",
    "description": "Annotate information on git interactive rebase.",
    "version": "0.2.0",
    "publisher": "softwareape",
    "license": "MIT",
    "readme": "README.md",
    "icon": "icon.png",
    "repository": {
        "url": "https://github.com/SoftwareApe/vscode-rebaser",
        "type": "git"
    },
    "engines": {
        "vscode": "^1.43.2"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:git-rebase"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "rebaser.changeRebaseCommand",
                "title": "Change type of rebase command between pick, reword, edit, etc. "
            }
        ],
        "keybindings": [
            {
                "key": "p",
                "command": "rebaser.changeRebaseCommand",
                "args": {
                    "text" : "p"
                },
                "when": "editorTextFocus && editorLangId == git-rebase"
            },
            {
                "key": "r",
                "command": "rebaser.changeRebaseCommand",
                "args": {
                    "text" : "r"
                },
                "when": "editorTextFocus && editorLangId == git-rebase"
            },
            {
                "key": "e",
                "command": "rebaser.changeRebaseCommand",
                "args": {
                    "text" : "e"
                },
                "when": "editorTextFocus && editorLangId == git-rebase"
            },
            {
                "key": "s",
                "command": "rebaser.changeRebaseCommand",
                "args": {
                    "text" : "s"
                },
                "when": "editorTextFocus && editorLangId == git-rebase"
            },
            {
                "key": "f",
                "command": "rebaser.changeRebaseCommand",
                "args": {
                    "text" : "f"
                },
                "when": "editorTextFocus && editorLangId == git-rebase"
            },
            {
                "key": "x",
                "command": "rebaser.changeRebaseCommand",
                "args": {
                    "text" : "x"
                },
                "when": "editorTextFocus && editorLangId == git-rebase"
            },
            {
                "key": "d",
                "command": "rebaser.changeRebaseCommand",
                "args": {
                    "text" : "d"
                },
                "when": "editorTextFocus && editorLangId == git-rebase"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.37",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
