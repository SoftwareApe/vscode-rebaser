{
    "name": "rebaser",
    "displayName": "rebaser",
    "description": "Annotate information on git interactive rebase.",
    "version": "0.1.1",
    "publisher": "softwareape",
    "license": "MIT",
    "readme": "README.md",
    "icon": "icon.png",
    "repository": {
        "url": "https://github.com/SoftwareApe/vscode-rebaser",
        "type": "git"
    },
    "engines": {
        "vscode": "^1.43.2"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:git-rebase"
    ],
    "main": "./out/extension",
    "contributes": {
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.37",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
